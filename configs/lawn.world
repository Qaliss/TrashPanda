<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="lawn">

    <!-- Lighting -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Ground -->
    <model name="lawn">
      <static>true</static>
      <link name="lawn_link">
        <visual name="lawn_visual">
          <geometry>
            <box>
              <size>10 10 0.01</size>
            </box>
          </geometry>
        </visual>
        <collision name="lawn_collision">
          <geometry>
            <box>
              <size>10 10 0.01</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>

